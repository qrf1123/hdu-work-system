AAA,CAAA,GAAA,CAAA,EAAA;AADA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,uBAAA,EAAA;AAAA,kBAAA,SAAA,EAAA;AAAA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAGA,oBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAlBA;AAmBA,IAAA,OAnBA,qBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,mCAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,kBAAA,QADA,GACA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,WADA;AAEA,kBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,2BAAA,IAAA,CAAA,SAAA,KAAA,GAAA;AACA,mBAFA,CAAA;AAGA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA3BA;AA4BA,IAAA,YA5BA,wBA4BA,SA5BA,EA4BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,SAAA,EAAA;AAAA;AAFA,OAAA;AAIA;AAjCA,GATA;AA4CA,EAAA,OA5CA,qBA4CA;AACA,SAAA,OAAA;AACA;AA9CA,CAAA","sourcesContent":["<template>\n    <van-tab title=\"签到签退\">\n        <ul class=\"list\">\n            <list-layout v-for=\"(item,index) in myJob\" :key=\"index\" :firstLayout=\"1\">\n                <van-icon size=\"40\" color=\"rgb(81, 125, 252)\" name=\"clock-o\" slot=\"left-icon\"/>\n                <div slot=\"content\">\n                    <h4>{{item.stationSubject}}</h4>\n                    <div>\n                        <span>{{item.teacher}}</span>\n                    </div>\n                </div>\n                <div class=\"sign\" slot=\"right\">\n                    <span class=\"date\">{{item.operateDate}}</span>\n                    <van-button @click=\"handleSignIn(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">签到</van-button>\n                    <van-button @click=\"handleSignOut(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">签退</van-button>\n                    <van-button @click=\"handleResign(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">补签</van-button>\n                </div>\n            </list-layout>\n        </ul>\n    </van-tab>\n</template>\n\n<script>\nimport ListLayout from '@/components/common/ListLayout.vue'\nexport default {\n    data() {\n        return {\n            myJob: {}\n        }\n    },\n    components: {\n        ListLayout\n    },\n    methods: {\n        async handleSignIn(id) {\n            console.log(id)\n            const res = await this.$ajax.post('/system/hours/signIn',{stationId: id})\n            if(res.data.code === 200) {\n                this.$toast(res.data.msg)\n            }else {\n                this.$toast('出错了，请稍后重试~')\n            }\n            console.log(res.data)\n        },\n        async handleSignOut(id) {\n            console.log(id)\n            const res = await this.$ajax.post('/system/hours/signOut',{stationId: id})\n            if(res.data.code === 200) {\n                this.$toast(res.data.msg)\n            }\n            console.log(res.data)\n        },\n        async getList() {\n            const res = await this.$ajax.get('/system/stationStudent/queryMyJob')\n            if(res.data.code === 200) {\n                const stations = res.data.data.stationData\n                this.myJob = stations.filter(item => {\n                    return item.stateCode === '1'\n                })\n            }\n        },\n        handleResign(stationId) {\n            this.$router.push({\n                path: '/resign',\n                query: {stationId: stationId}\n            })\n        }\n    },\n    mounted() {\n        this.getList()\n    }\n}\n</script>\n\n<style scoped>\n    .list .sign .van-button:nth-child(3) {\n        right: 80px;\n    }\n    .list .sign .van-button:nth-child(4) {\n        right: 150px;\n    }\n</style>"],"sourceRoot":"src/views/student/checkWork"}]}