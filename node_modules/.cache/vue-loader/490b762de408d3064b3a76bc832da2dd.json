       <span>{{item.teacher}}</span>\n                            <span>{{item.totalNum}}人</span>\n                        </div>\n                    </div>\n                    <div slot=\"right\">\n                        <span class=\"date\">{{item.operateDate}}</span>\n                        <van-button :disabled=\"item.stateCode !== '0'\" @click=\"handleEmploy(index)\" size=\"mini\" color=\"rgb(81, 125, 252)\">{{item.state}}</van-button>\n                    </div>\n                </list-layout>\n            </van-list>\n        </ul>\n    </van-tab>\n</template>\n\n<script>\nimport Search from '@/components/common/Search.vue'\nimport ListLayout from '@/components/common/ListLayout.vue'\nexport default {\n    data() {\n        return {\n            list: [],\n            pageNum: 1,\n            loading: false,\n            finished: false\n        }\n    },\n    components: {\n        Search,\n        ListLayout\n    },\n    methods: {\n        onLoad() {\n            setTimeout(() => {\n                this.pageNum++\n                this.getList()\n                this.loading = false\n            },1000)\n        },\n        handleSearch(val) {\n            console.log(val)\n        },\n        handleDetail(stationId) {\n            this.$router.push({name: 'JobDetail', params: {stationId: stationId}})\n        },\n        handleEmploy(index) {\n            console.log(index)\n        },\n        async getList() {\n            const res = await this.$ajax.get(`/system/stationStudent/queryMyJob?pageNum=${this.pageNum}&pageSize=7`)\n            if(res.data.code === 200) {\n                const list = res.data.data.stationData\n                this.list.push(...list)\n                if(list.length < 7) {\n                    this.finished = true\n                }\n            }\n        }\n    },\n    mounted() {\n        this.getList()\n    }\n}\n</script>\n\n<style scoped>\n.van-row .van-col--9:nth-child(2){\n    margin-top: 10px;\n    padding-left: 1px;\n}\n\n.van-col--9 .van-search {\n    height: 36px;\n    padding: 3px;\n}\n\n.van-dropdown-menu >>> .van-dropdown-menu__bar {\n    height: 36px;\n    box-shadow: 0 0 0 #fff;\n}\n.van-button {\n    margin-top: 13px;\n    margin-left: 15px;\n}\n\n</style>"]}]}