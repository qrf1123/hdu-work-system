        <div class=\"sign\" slot=\"right\">\n                    <span class=\"date\">{{item.operateDate}}</span>\n                    <van-button @click=\"handleSignIn(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">签到</van-button>\n                    <van-button @click=\"handleSignOut(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">签退</van-button>\n                    <van-button @click=\"handleResign(item.stationId)\" size=\"mini\" color=\"rgb(81, 125, 252)\">补签</van-button>\n                </div>\n            </list-layout>\n        </ul>\n    </van-tab>\n</template>\n\n<script>\nimport ListLayout from '@/components/common/ListLayout.vue'\nexport default {\n    data() {\n        return {\n            myJob: {}\n        }\n    },\n    components: {\n        ListLayout\n    },\n    methods: {\n        async handleSignIn(id) {\n            console.log(id)\n            const res = await this.$ajax.post('/system/hours/signIn',{stationId: id})\n            if(res.data.code === 200) {\n                this.$toast(res.data.msg)\n            }else {\n                this.$toast('出错了，请稍后重试~')\n            }\n            console.log(res.data)\n        },\n        async handleSignOut(id) {\n            console.log(id)\n            const res = await this.$ajax.post('/system/hours/signOut',{stationId: id})\n            if(res.data.code === 200) {\n                this.$toast(res.data.msg)\n            }\n            console.log(res.data)\n        },\n        async getList() {\n            const res = await this.$ajax.get('/system/stationStudent/queryMyJob')\n            if(res.data.code === 200) {\n                const stations = res.data.data.stationData\n                this.myJob = stations.filter(item => {\n                    return item.stateCode === '1'\n                })\n            }\n        },\n        handleResign(stationId) {\n            this.$router.push({\n                path: '/resign',\n                query: {stationId: stationId}\n            })\n        }\n    },\n    mounted() {\n        this.getList()\n    }\n}\n</script>\n\n<style scoped>\n    .list .sign .van-button:nth-child(3) {\n        right: 80px;\n    }\n    .list .sign .van-button:nth-child(4) {\n        right: 150px;\n    }\n</style>"]}]}