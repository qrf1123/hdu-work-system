eadonly clickable name=\"calendar\" :value=\"model.endDate\"  placeholder=\"点击选择日期\" @click=\"showCalendar2 = true\"/>\n            <van-calendar color=\"rgb(81, 125, 252)\" v-model=\"showCalendar2\" @confirm=\"onConfirmEnd\" />\n        </div>\n        <van-collapse v-model=\"activeNames\">\n            <van-collapse-item name=\"1\">\n                <template #title>\n                    <div class=\"title\">\n                        <div>\n                            <span class=\"disc\"></span>\n                            <h4>实习辅导员</h4>\n                            <van-button color=\"rgb(81, 125, 252)\" @click=\"handleSubmit\" size=\"mini\">提交名单</van-button>\n                        </div>\n                        <p>\n                            <span>总人数：5人</span>\n                        </p>\n                    </div>\n                </template>\n                <ul class=\"studentList\">\n                    <li>姓名：哈哈哈</li>\n                    <li>姓名：嘿嘿嘿</li>\n                    <li>姓名：嘻嘻嘻</li>\n                </ul>\n            </van-collapse-item>\n        </van-collapse>\n        <ul class=\"list\">\n            <list-layout :firstLayout=\"1\" v-for=\"(item,index) in list\" :key={index}>\n                <van-icon name=\"contact\" size=\"40\" color=\"rgb(81, 125, 252)\" dot slot=\"left-icon\" />\n                <div slot=\"content\">\n                    <h4>{item.studentName}</h4>\n                    <div class=\"info\">\n                        <!-- <p>总人数：5人</p> -->\n                        <!-- <p>开始时间：13:00</p>\n                        <p>结束时间：15:00</p> -->\n                        <p>工时：{{item.workHours}}</p>\n                    </div>\n                </div>\n                <div class=\"right\" slot=\"right\">\n                    <span class=\"date\">{{item.workDate}}</span>\n                    <van-button :disabled=\"item.comfirmStatus === '已确认'\" size=\"mini\" color=\"rgb(81, 125, 252)\">{{item.comfirmStatus}}</van-button>\n                </div>\n            </list-layout>\n            <list-layout :firstLayout=\"1\">\n                <van-icon name=\"contact\" size=\"40\" color=\"rgb(81, 125, 252)\" dot slot=\"left-icon\" />\n                <div slot=\"content\">\n                    <h4>哈哈哈</h4>\n                    <div class=\"info\">\n                        <!-- <p>总人数：5人</p> -->\n                        <p>开始时间：13:00</p>\n                        <p>结束时间：15:00</p>\n                        <p>工时：2h</p>\n                    </div>\n                </div>\n                <div class=\"right\" slot=\"right\">\n                    <span class=\"date\">2020-06-13</span>\n                    <van-button size=\"mini\" color=\"rgb(81, 125, 252)\">确认</van-button>\n                    <van-button plain @click=\"handleChangeTime()\" size=\"mini\" color=\"rgb(81, 125, 252)\">修改</van-button>\n                </div>\n            </list-layout>\n        </ul>\n    </div>\n</template>\n\n<script>\nimport TopNav from '@/components/common/TopNav.vue'\nimport BottomNav from '@/components/teacher/BottomNav.vue'\nimport ListLayout from '@/components/common/ListLayout.vue'\n/*\n * 注：// 这个模块的接口好像有问题喔，暂时没有功能\n*/\nexport default {\n    data() {\n        return {\n            model: {\n                startDate: '',\n                endDate: ''\n            },\n            list: [],\n            showCalendar1: false,\n            showCalendar2: false,\n            checked: false,\n            activeNames: [''],\n        }\n    },\n    components: {\n        TopNav,\n        BottomNav,\n        ListLayout\n    },\n    methods: {\n        onConfirmStart(date) {\n            this.model.startDate = `${date.getMonth() + 1}/${date.getDate()}`;\n            this.showCalendar1 = false;\n        },\n        onConfirmEnd(date) {\n            this.model.endDate = `${date.getMonth() + 1}/${date.getDate()}`;\n            this.showCalendar2 = false;\n        },\n        handleChangeTime() {\n            this.$router.push('/changeTime')\n        },\n        handleSubmit() {\n            console.log('提交名单')\n        },\n        async getList() {\n            // param测试使用的\n            const param = {beginDate: '2020-04-01', endDate: '2020-05-01'}\n            const res = await this.$ajax.get('/system/hours/workHoursStatics',{params: param})\n            console.log(res.data)\n            if(res.data.code === 200) {\n                this.list = res.data.data\n            }\n        }\n    },\n    mounted() {\n        this.getList()\n    }\n}\n</script>\n\n<style scoped>\n    .dateSelect {\n        background-color: #fff;\n        padding: 10px 30px;\n    }\n    .dateSelect .van-field {\n        border: 1px solid #eee;\n        border-radius: 5px;\n    }\n    .dateSelect .van-field>>>.van-icon {\n        color: rgb(81, 125, 252);\n    }\n    .dateSelect .van-field:first-child {\n        margin-bottom: 10px;\n    }\n    .van-collapse {\n        margin: 10px auto;\n    }\n    .title div {\n        position: relative;\n        display: flex;\n        align-items: center;\n    }\n    .title div h4 {\n        margin: 0 10px;\n    }\n    .title .disc {\n        background-color: rgb(81, 125, 252);\n        width: 10px;\n        height: 10px;\n        border-radius: 50%;\n    }\n    .title div .van-button {\n        position: absolute;\n        right: 5px;\n        top: 0;\n    }\n    .title p {\n        margin-left: 20px;\n        color: #555;\n    }\n    .studentList li {\n        height: 20px;\n        line-height: 20px;\n        font-size: 14px;\n        color: #444;\n    }\n    .info p {\n        font-size: 14px;\n        color: #555;\n    }\n    .right .van-button:nth-child(3) {\n        right: 80px;\n    }\n</style>"]}]}